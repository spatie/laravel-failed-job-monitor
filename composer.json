{
    "name": "spatie/laravel-failed-job-monitor",
    "description": "Get notified when a queued job fails",
    "keywords": [
        "spatie",
        "queue",
        "job",
        "monitor",
        "notify",
        "slack",
        "laravel-failed-job-monitor"
    ],
    "homepage": "https://github.com/spatie/laravel-failed-job-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/queue": "~5.3.8",
        "illuminate/notifications": "~5.3.8"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.5.4",
        "orchestra/testbench":"~3.3.1",
        "laravel/framework":"~5.3.8"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\FailedJobMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\FailedJobMonitor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows notifications to be sent via Slack"
    }
}
